{
  "manifest_version": 3,
  "name": "Ceddo Skipper",
  "version": "1.0.0",
  "description": "A Chrome extension for skipping content",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://*.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "jszip.min.js",
        "ort.min.js",
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["nn/trained_model.onnx", "nn/trained_model_mobilenet.onnx", "ort-wasm-simd.wasm", "ort-wasm.wasm"],
      "matches": ["https://*.youtube.com/*"]
    }
  ]
}