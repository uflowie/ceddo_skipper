{
  "manifest_version": 3,
  "name": "Ceddo Skipper",
  "version": "1.0.0",
  "description": "A Chrome extension for skipping content",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://*.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}